class Cow : public Farm_Animal
{
public:
    Cow(string name,string feature,string sound,vector<string>resource): Farm_Animal(name,feature,sound, resource){};
    void get_action(string object)
    {
        cout<<name<<' '<< feature<<' '<< object<<endl;
    }
};
class Chicken : public Farm_Animal
{
public:
    Chicken(string name,string feature,string sound,vector<string>resource): Farm_Animal(name,feature,sound, resource){

    };
    void get_action(string object)
    {
        cout<<name<<' '<< feature<<' '<< object<<endl;
    }
};
